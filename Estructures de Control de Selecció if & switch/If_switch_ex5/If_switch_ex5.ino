//****************************VARIABLE*************************************

float Carboni = 1;


//****************************SETUP****************************************

void setup() {                 //configura el final de salida 
Serial.begin(9600); 

} 

//****************************LOOP*****************************************
void loop() {                 //configura el bucle 
  if ( Carboni < 3.5)
  {
    Serial.print("A"); 
  }
  else if ( Carboni <= 6.5 && Carboni > 3.5)
  {
    Serial.print("B");
  }
}

